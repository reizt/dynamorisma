{
	"name": "dynmr",
	"version": "2.0.0-alpha.8",
	"repository": "git@github.com:reizt/dynmr.git",
	"author": "reizt <reizt.dev@gmail.com>",
	"license": "MIT",
	"main": "./lib/cjs/index.js",
	"types": "./lib/types/index.d.ts",
	"module": "./lib/esm/index.mjs",
	"exports": {
		".": {
			"import": "./lib/esm/index.js",
			"require": "./lib/cjs/index.js",
			"types": "./lib/types/index.d.ts"
		},
		"./package.json": "./package.json"
	},
	"files": ["lib/cjs/**/*.js", "lib/esm/**/*.js", "lib/types/**/*.d.ts"],
	"scripts": {
		"build": "run-s cleanup build.all",
		"cleanup": "rm -rf lib/*",
		"build.all": "run-p build.cjs build.esm build.types",
		"build.cjs": "tsc --project tsconfig.cjs.json",
		"build.esm": "tsc --project tsconfig.esm.json",
		"build.types": "tsc --project tsconfig.types.json",
		"format": "biome check --apply .",
		"check": "run-p check.*",
		"check.biome": "biome check .",
		"check.tsc": "tsc --incremental --noEmit",
		"check.test": "vitest run",
		"test": "vitest watch"
	},
	"dependencies": {
		"@aws-sdk/client-dynamodb": "^3.606.0",
		"yargs": "^17.7.2"
	},
	"devDependencies": {
		"@biomejs/biome": "^1.7.3",
		"@types/node": "^20.12.11",
		"@types/yargs": "^17.0.32",
		"lefthook": "^1.6.10",
		"npm-run-all": "^4.1.5",
		"typescript": "^5.4.5",
		"vitest": "^1.6.0"
	}
}
