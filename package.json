{
	"name": "dynmr",
	"version": "1.8.5",
	"repository": "git@github.com:reizt/dynmr.git",
	"author": "reizt <reizt.dev@gmail.com>",
	"license": "MIT",
	"type": "commonjs",
	"main": "./cjs/index.js",
	"module": "./esm/index.js",
	"types": "./types/index.d.ts",
	"exports": {
		".": {
			"import": {
				"types": "./types/index.d.ts",
				"default": "./esm/index.js"
			},
			"require": {
				"types": "./types/index.d.ts",
				"default": "./cjs/index.js"
			},
			"default": "./cjs/index.js"
		},
		"./package.json": "./package.json"
	},
	"scripts": {
		"build": "run-s clean build:* set-type:* cp-pkg",
		"build:cjs": "node --no-warnings ./esbuild.mjs cjs",
		"build:esm": "node --no-warnings ./esbuild.mjs esm",
		"build:types": "tsc --project tsconfig.tsc.json",
		"set-type:cjs": "echo '{\"type\": \"commonjs\"}' > .pkg/cjs/package.json",
		"set-type:esm": "echo '{\"type\": \"module\"}'   > .pkg/esm/package.json",
		"cp-pkg": "cp package.json .pkg",
		"clean": "for f in .pkg/*; do rm -rf $f; done",
		"check-all": "run-p typecheck fmt.check",
		"fmt": "biome check --apply .",
		"fmt.check": "biome check .",
		"typecheck": "tsc --incremental --noEmit"
	},
	"dependencies": {
		"@aws-sdk/client-dynamodb": "^3.574.0",
		"yargs": "^17.7.2"
	},
	"devDependencies": {
		"@biomejs/biome": "^1.7.3",
		"@types/node": "^20.12.11",
		"@types/yargs": "^17.0.32",
		"esbuild": "^0.21.1",
		"lefthook": "^1.6.10",
		"nodemon": "^3.1.0",
		"npm-run-all": "^4.1.5",
		"typescript": "^5.4.5",
		"vitest": "^1.6.0"
	}
}
